{
  "name": "rest-loader-control",
  "version": "1.0.0",
  "description": "An Azure DevOps Work Item Control for loading values from a Rest endpoint",
  "main": "index.js",
  "scripts": {
    "build-dev": "webpack -p --mode=development",
    "package-dev": "tfx extension create --manifest-globs azure-devops-extension.json --overrides-file azure-devops-extension-dev.json",
    "serve-dev": "webpack-dev-server --cert localhost.crt --key localhost.key",
    "watch": "webpack --watch",
    "build": "webpack -p --mode=production",
    "postbuild": "npm run package",
    "package": "tfx extension create --manifest-globs azure-devops-extension.json",
    "gallery-publish": "tfx extension publish --rev-version",
    "clean": "rimraf ./dist && rimraf ./*.vsix",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "Richard Goforth",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "azure-devops-extension-api": "^1.152.3",
    "azure-devops-extension-sdk": "^2.0.10",
    "azure-devops-ui": "^1.160.4",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.135",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.2",
    "typescript-styled-plugin": "^0.14.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.11.0"
  }
}
